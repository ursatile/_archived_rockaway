@model IEnumerable<Rockaway.WebApp.Data.Entities.Show>

@{
	ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
	<a asp-action="Create">Create New</a>
</p>
<table class="table">
	<thead>
		<tr>
			<th>Venue</th>
			<th>Date</th>
			<th>Headliner</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model) {
			<tr>
				<td>@item.Venue.Name</td>
				<td>@item.Date</td>
				<td>@item.HeadlineArtist.Name</td>
				<td>
					@Html.ActionLink("Edit", "Edit", item.RouteValues) |
					@Html.ActionLink("Details", "Details", item.RouteValues) |
					@Html.ActionLink("Delete", "Delete", item.RouteValues)
				</td>
			</tr>
		}
	</tbody>
</table>
